---
import Layout from '../layouts/Layout.astro';
const pageTitle = "Blog page";
const allPosts = await Astro.glob('../pages/posts/*.md');

console.log(allPosts);

---

<Layout pageTitle={pageTitle}>
	<h1>Blog page</h1>
	<ul>
		{allPosts.map(post => {
			return (
				<h3>
					<a href={post.url}>{post.frontmatter.title}</a>
				</h3>
			);
		})}
	</ul>

</Layout>

